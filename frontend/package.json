    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@vue/apollo-composable": "^4.2.2",
    "autoprefixer": "^10.4.21",
    "graphql": "^16.12.0",
    "graphql-ws": "^6.0.6",
    "idb": "^8.0.3",
    "pinia": "^3.0.4",
    "primeicons": "^7.0.0",
    "primelocale": "^2.2.3",
    "primevue": "^4.5.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.24",
    "vue-draggable-plus": "^0.6.1",
